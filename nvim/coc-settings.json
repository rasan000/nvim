{
    "coc.preferences.extensionUpdateCheck": "weekly",
    "suggest.autoTrigger": "always",
    "suggest.minTriggerInputLength": 2,
    "languageserver": {
        "terraform": {
            "command": "terraform-ls",
            "args": [
                "serve"
            ],
            "filetypes": [
                "terraform",
                "tf"
            ],
            "initializationOptions": {},
            "settings": {}
        },
        "kotlin": {
            "command": "coc/extensions/coc-kotlin-data/langServerInstall/server/bin/kotlin-language-server",
            "filetypes": [
                "kotlin"
            ]
        }
    }
}
